{
  "expo": {
    "name": "ecomerce",
    "slug": "wholesaledltd",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/image-3.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/rectangle-1@3x.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/image-3.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ecom"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/image-3.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5ff7b536-e012-4192-beb2-4fe2ee710b05"
      }
    },
    "owner": "youssef2001"
  }
}
