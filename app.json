{
  "expo": {
    "name": "ecomerce",
    "slug": "ecomerce",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/image-3.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/rectangle-1@3x.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/output-onlinepngtools.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ecom"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/image-3.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7f170675-0eba-4804-a15d-cce69d19fb6a"
      }
    }
  }
}
